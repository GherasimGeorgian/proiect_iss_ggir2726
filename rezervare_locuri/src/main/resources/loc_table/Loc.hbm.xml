<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Loc"
           table="Loc">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>

        <property
                name="randul"
                column="randul"/>
        <property
                name="loja"
                column="loja"/>
        <property
                name="numar"
                column="numar"/>
        <property
                name="pret"
                column="pret"/>

        <property
                name="stare"
                column="stare"/>
        <property
                name="id_reprezentatie"
                column="id_reprezentatie"/>

    </class>
</hibernate-mapping>