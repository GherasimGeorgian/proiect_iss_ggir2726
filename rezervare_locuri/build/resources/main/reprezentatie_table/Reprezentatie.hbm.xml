<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Reprezentatie"
           table="Reprezentatie">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>

        <property
                name="nume"
                column="nume"/>
        <property
                name="rating"
                column="rating"/>
        <property
                name="durata"
                column="durata"/>

    </class>
</hibernate-mapping>