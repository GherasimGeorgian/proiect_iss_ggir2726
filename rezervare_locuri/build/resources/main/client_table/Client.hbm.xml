<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Client"
           table="Client">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>

        <property
                name="userName"
                column="userName"/>
        <property
                name="password"
                column="password"/>

    </class>
</hibernate-mapping>